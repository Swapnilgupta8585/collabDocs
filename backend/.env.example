# backend/.env.example

# Server port
PORT="5000"

# PostgreSQL connection URL
# Format: postgres://<user>:<pass>@<host>:<port>/<dbname>?sslmode=<mode>
DB_URL="postgres://db_user:db_pass@localhost:5432/collabdoc?sslmode=disable"

# JWT secret for signing access & refresh tokens
SECRET_TOKEN="your_jwt_secret_here"

# Platform indicator (e.g. dev, prod)
PLATFORM="dev"
